<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ESTaskFinishButton">
        <attr name="finish" format="boolean" />
    </declare-styleable>

    <!--水平4个按钮-->
    <declare-styleable name="TagsLayout">
        <attr name="tagVerticalSpaces" format="dimension" />
        <attr name="tagHorizontalSpaces" format="dimension" />
    </declare-styleable>

    <declare-styleable name="EduSohoImageText">
        <attr name="EduSohoImageText_text" format="string" />
        <attr name="EduSohoImageText_image_normal" format="reference" />
        <attr name="EduSohoImageText_image_pressed" format="reference" />
        <attr name="EduSohoImageText_text_size" format="dimension" />
        <attr name="EduSohoImageText_image_width" format="dimension" />
        <attr name="EduSohoImageText_image_height" format="dimension" />
        <attr name="EduSohoImageText_text_color_normal" format="color" />
        <attr name="EduSohoImageText_text_color_pressed" format="color" />
    </declare-styleable>

    <declare-styleable name="CourseMenuButton">
        <attr name="button_image" format="string" />
        <attr name="image_background" format="integer" />
        <attr name="image_color" format="integer" />
        <attr name="image_size" format="dimension" />
        <attr name="button_text" format="string" />
        <attr name="button_text_size" format="dimension" />
        <attr name="button_text_color" format="integer" />
    </declare-styleable>

    <declare-styleable name="EduSohoTextBtn">
        <attr name="text" format="string" />
        <attr name="image" format="string" />
        <attr name="size" format="dimension" />
        <attr name="text_size" format="dimension" />
        <attr name="fontColor" format="color" />
        <attr name="iconSizeScale" format="float" />
    </declare-styleable>

    <declare-styleable name="ESExpandableTextView">
        <attr name="maxCollapsedLines" format="integer" />
        <attr name="animDuration" format="integer" />
        <attr name="animAlphaStart" format="float" />
        <attr name="expandDrawable" format="reference" />
        <attr name="collapseDrawable" format="reference" />
        <attr name="expandText" format="string" />
        <attr name="collapseText" format="string" />
    </declare-styleable>

    <declare-styleable name="EduSohoLoadingButton">
        <attr name="init_text" format="string" />
        <attr name="loading_text" format="string" />
        <attr name="success_text" format="string" />
        <attr name="init_background" format="integer" />
        <attr name="loading_background" format="integer" />
    </declare-styleable>

    <declare-styleable name="MuiltTextView">
        <attr name="muiltTitle" format="string" />
        <attr name="muiltColor" format="color" />
        <attr name="contentColor" format="color" />
        <attr name="isFirstPadding" format="boolean" />
    </declare-styleable>

    <declare-styleable name="FancyButtonsAttrs">
        <attr name="defaultColor" format="color" />
        <attr name="textStr" format="string" />

        <attr name="textColor" format="color" />

        <attr name="textFont" format="string" />
        <attr name="iconFont" format="string" />

        <attr name="textSize" format="dimension" />
        <attr name="iconResource" format="integer" />
        <attr name="fontIconResource" format="string" />
        <attr name="fontIconSize" format="dimension" />
        <attr name="iconPosition" format="enum">
            <enum name="left" value="1" />
            <enum name="right" value="2" />
            <enum name="top" value="3" />
            <enum name="bottom" value="4" />
        </attr>
        <attr name="borderColor" format="color" />
        <attr name="pba_borderWidth" format="dimension" />
        <attr name="focusColor" format="color" />
        <attr name="radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="lineSpacing" format="dimension" />
        <attr name="maxLine" format="integer" />
    </declare-styleable>

    <declare-styleable name="ESProgressBar">
        <attr name="percent_size" format="dimension" />
        <attr name="text_visible" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ESRadioGroup">
        <attr name="isMultiple" format="boolean" />
    </declare-styleable>


    <declare-styleable name="RatioView">
        <attr name="ratio" format="float" />
    </declare-styleable>

    <declare-styleable name="ESProjectPlanRecordItem">
        <attr name="title" format="string" />
        <attr name="icon" format="reference" />
    </declare-styleable>

</resources>